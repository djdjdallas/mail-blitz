{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["next/babel", "@babel/preset-react", "@babel/preset-env"]
    }
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}
